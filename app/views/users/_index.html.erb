<table class='table'>
  <thead>
    <tr>
      <th></th>
      <th>ニックネーム</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td>
          <%= attachment_image_tag user, :profile_image, size: "50x50", fallback: "user_no_image.png", class:"mt-3 rounded-circle"; %>
          <%= user.name %>
        </td>
        <td><%= link_to '詳細', user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
